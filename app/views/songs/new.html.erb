<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  <% Note.all.each do |note| %>
    <input name="song_notes_<%=note.id + 1%>[]">
  <% end %>

  <%= f.submit %>
<% end %>
